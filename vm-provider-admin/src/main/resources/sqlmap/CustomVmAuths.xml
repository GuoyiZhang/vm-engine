<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.vm.admin.dao.mapper.custom.CustomVmAuthsMapper">
    <select id="getMenusByIds" resultMap="com.vm.admin.dao.mapper.VmAuthsMapper.VmAuthsMap">
        SELECT
        *
        FROM
        vm_auths
        WHERE
        is_deleted=#{query.isDeleted}
        and status=#{query.status}
        and id IN
        <foreach item="item" index="index" collection="query.authIds" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>
</mapper>