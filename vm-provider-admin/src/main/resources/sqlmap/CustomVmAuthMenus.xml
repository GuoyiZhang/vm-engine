<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.vm.admin.dao.mapper.custom.CustomVmAuthMenusMapper">
    <select id="getMenusByIds" resultMap="com.vm.admin.dao.mapper.VmAuthMenusMapper.VmAuthMenusMap">
        SELECT
        *
        FROM
        vm_auth_menus
        WHERE
        is_deleted=#{query.isDeleted}
        and status=#{query.status}
        and id IN
        <foreach item="item" index="index" collection="query.menuIds" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>
</mapper>