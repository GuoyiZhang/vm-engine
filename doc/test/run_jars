cp ../../vm-eureka/target/vm-eureka-1.0.0.jar ./jars/
cp ../../vm-config-server/target/vm-config-server-1.0.0.jar ./jars/
cp ../../vm-gateway/target/vm-gateway-1.0.0.jar ./jars/
cp ../../vm-provider-admin/target/vm-provider-admin-1.0.0.jar ./jars/
cp ../../vm-provider-user/target/vm-provider-user-1.0.0.jar ./jars/
cp ../../vm-provider-movie/target/vm-provider-movie-1.0.0.jar ./jars/


java -jar ./jars/vm-eureka-1.0.0.jar --spring.profiles.active=dev00 -Xms24m -Xmx48m -Xmn24m >./outs/eureka00.out
